{
  "name": "lesphp/property-info-typed-array",
  "description": "Extract key and value types using PHP Attributes for array class properties. This dispense the use of PhpDocExtractor for typed arrays.",
  "keywords": [
    "symfony",
    "property-info",
    "array",
    "typed"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "LesPhp\\PropertyInfo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LesPhp\\PropertyInfo\\Tests\\" : "tests/"
    }
  },
  "require": {
    "php": ">=8.0.2",
    "symfony/property-info": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.8"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "6.0.x-dev"
    }
  }
}
